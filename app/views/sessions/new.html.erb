<h1>Log In</h1>
<%= form_tag(sessions_path) do %>
    <div class="field">
        <%= label_tag :email %>
        <%= text_field_tag :email, '', :class => "", :placeholder => "Email"%>
    </div>

    <div class="field">
        <%= label_tag :password %>
        <%= password_field_tag :password, '', :class => "", :placeholder => "Password"%>
    </div>

    <div class="actions">
        <%= submit_tag "Login" %>   
    </div>

<% end%>

<p><%= link_to "Github Login", "/auth/github" %></p>